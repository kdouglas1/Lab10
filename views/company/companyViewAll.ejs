<html>
<head>
    <title>Company</title>
</head>
<body>
<h1>Company</h1>
<table border="1">
    <tr><th>Company ID</th><th>Name</th><th>company_name</th></tr>
    <% for(var i=0; result.length > i; i++ ) { %>
    <tr>
        <td><%= result[i].company_id%></td>
        <td><a href="/company/?company_id=<%= result[i].company_id %>"><%= result[i].company_name %></a></td>
        <td><%= result[i].company_name %></td>
    </tr>
    <% } %>
</table>
</body>

</html>